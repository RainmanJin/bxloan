<!--意见-->
<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"
%>
<div id="faq-list-3" class="panel-group accordion-style1 accordion-style2">
	<div class="step-content pos-rel" id="step-container">
		<div class="step-pane active" id="step1">
			<form class="form-horizontal" id="form-suggestion">
					<div>
						<!-- 意见输入 -->
					<div class="form-group">
						<label for="fulfillInstructionCd" class="col-xs-12 col-sm-2 control-label no-padding-right">
							合同条件落实情况说明:
						</label>
						<div class="col-xs-12 col-sm-8">
							<span class="block input-icon input-icon-right">
								<textarea rows="5" cols="45" id="fulfillInstructionCd" name="fulfillInstructionCd" class="form-control"></textarea>
							</span>
						</div>
						<div class="help-block col-xs-12 col-sm-reset inline">
						</div>
					</div>
					<div class="form-group pre-opinion">
						<label for="opinion" class="col-xs-12 col-sm-2 control-label no-padding-right">
							意见:
						</label>
						<div class="col-xs-12 col-sm-8">
							<span class="block input-icon input-icon-right">
								<textarea rows="5" cols="45" id="opinion" name="opinion" class="form-control"></textarea>
							</span>
						</div>
						<div class="help-block col-xs-12 col-sm-reset inline">
						</div>
					</div>
				</div>
		</form>
		<div class="wizard-actions" style="text-align: center;margin-top: 10px;">
			<hr/>
			<button class="btn btn-sm btn-yellow" id="downloadContractContent" type="button">
				<i class="ace-icon fa fa-download">
				</i>
				下载合同文本
			</button>
			<button class="btn btn-sm btn-info" id="uploadContractContent" type="button">
				<i class="ace-icon fa fa-upload">
				</i>
				上传合同文本
			</button>
			<button class="btn btn-sm btn-primary" id="submitApply" type="button" data-loading-text="正在提交..">
				<i class="ace-icon fa fa-arrow-right">
				</i>
				提交
			</button>
			<button class="btn btn-sm btn-danger btn-next" id="dismissAppr" data-last="Finish" >
				<i class="ace-icon fa fa-floppy-o " data-loading-text="正在拒绝..">
				</i>
				拒绝
			</button>
			<button class="btn btn-sm btn-pre" id="goBackButton" data-last="Finish" type="button" onclick="javascript:history.go(-1);">
				<i class="ace-icon fa fa-chevron-left">
				</i>
				返回
			</button>
		</div>
		</div>
	</div>
</div>