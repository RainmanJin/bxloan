package com.coamctech.bxloan.service.model;

import java.sql.Timestamp;

public class ProcessSuggestionVO {

	private String comments; //意见
	private String assigner; //接受人
	private String submitTime; //提交时间
	private String stepId; //步骤Id
	public ProcessSuggestionVO(String comments, String assigner,
			String submitTime, String stepId) {
		super();
		this.comments = comments;
		this.assigner = assigner;
		this.submitTime = submitTime;
		this.stepId = stepId;
	}
	public ProcessSuggestionVO() {
		super();
	}
	public String getComments() {
		return comments;
	}
	public void setComments(String comments) {
		this.comments = comments;
	}
	public String getAssigner() {
		return assigner;
	}
	public void setAssigner(String assigner) {
		this.assigner = assigner;
	}
	public String getSubmitTime() {
		return submitTime;
	}
	public void setSubmitTime(String submitTime) {
		this.submitTime = submitTime;
	}
	public String getStepId() {
		return stepId;
	}
	public void setStepId(String stepId) {
		this.stepId = stepId;
	}
	
	
	
	
}
