package com.coamctech.bxloan.service.model.userinfo;

public class UserInfoUpdateVO extends UserInfoDetailVO {

	protected Long personId;
	protected String logpass;
	protected String newPass;
	protected String repeatNewPass;
	
	///////////////////////
	////GETTERS&SETTERS///
	//////////////////////
	public Long getPersonId() {
		return personId;
	}
	public void setPersonId(Long personId) {
		this.personId = personId;
	}
	public String getLogpass() {
		return logpass;
	}
	public void setLogpass(String logpass) {
		this.logpass = logpass;
	}
	public String getNewPass() {
		return newPass;
	}
	public void setNewPass(String newPass) {
		this.newPass = newPass;
	}
	public String getRepeatNewPass() {
		return repeatNewPass;
	}
	public void setRepeatNewPass(String repeatNewPass) {
		this.repeatNewPass = repeatNewPass;
	}
	
}
