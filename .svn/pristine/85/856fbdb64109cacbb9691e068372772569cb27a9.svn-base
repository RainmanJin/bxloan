package com.coamctech.bxloan.web.vo.bizapply;

import java.math.BigDecimal;

public class ExpectCashFlowSimpleVo {
	private Long month;
	private BigDecimal familyCost;
	private BigDecimal income;
	private BigDecimal cost;
	private BigDecimal otherLoanRepayment;
	private Long id;
	public Long getMonth() {
		return month;
	}
	public void setMonth(Long month) {
		this.month = month;
	}
	public BigDecimal getFamilyCost() {
		return familyCost;
	}
	public void setFamilyCost(BigDecimal familyCost) {
		this.familyCost = familyCost;
	}
	public BigDecimal getIncome() {
		return income;
	}
	public void setIncome(BigDecimal income) {
		this.income = income;
	}
	public BigDecimal getCost() {
		return cost;
	}
	public void setCost(BigDecimal cost) {
		this.cost = cost;
	}
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	
	private ExpectCashFlowSimpleVo(Long month, BigDecimal familyCost,
			BigDecimal income, BigDecimal cost, BigDecimal otherLoanRepayment,
			Long id) {
		super();
		this.month = month;
		this.familyCost = familyCost;
		this.income = income;
		this.cost = cost;
		this.otherLoanRepayment = otherLoanRepayment;
		this.id = id;
	}
	public ExpectCashFlowSimpleVo() {
		super();
	}
	public BigDecimal getOtherLoanRepayment() {
		return otherLoanRepayment;
	}
	public void setOtherLoanRepayment(BigDecimal otherLoanRepayment) {
		this.otherLoanRepayment = otherLoanRepayment;
	}
	
	
}
