package com.coamctech.bxloan.service.model.approval;

import java.io.Serializable;
import java.math.BigDecimal;

public class ElApprInfoVo implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 2691793512806087838L;
	/**
	 * 
	 */
	private Long workflowId;
	private Long taskId;
	private Long projectId;
	private String taskStageCode;
	private Long curUserId;
	private String curUserLoginName;
	private String curUserName;
	private Long curUserOrgId;
	private Long curUserRoleId;
	private String nextUser;
	private String nextUserOrgId;
	//TODO 建议
	/**
	 * 建议金额
	 */
	private BigDecimal proposalAmt;
	/**
	 * 建议期限
	 */
	private Integer proposalTerm;
	/**
	 * 建议期限单位
	 */
	private String proposalTermUnit;
	/**
	 * 建议利率
	 */
	private BigDecimal proposalRate;
	//审批意见
	/**
	 * 
	 */
	private String comments;
	
	public ElApprInfoVo() {
		super();
	}
	
	public ElApprInfoVo(Long workflowId, Long taskId, Long projectId,
			Long curUserId, String nextUser) {
		super();
		this.workflowId = workflowId;
		this.taskId = taskId;
		this.projectId = projectId;
		this.curUserId = curUserId;
		this.nextUser = nextUser;
	}

	public Long getWorkflowId() {
		return workflowId;
	}

	public void setWorkflowId(Long workflowId) {
		this.workflowId = workflowId;
	}

	public Long getTaskId() {
		return taskId;
	}

	public void setTaskId(Long taskId) {
		this.taskId = taskId;
	}

	public Long getProjectId() {
		return projectId;
	}

	public void setProjectId(Long projectId) {
		this.projectId = projectId;
	}

	public Long getCurUserId() {
		return curUserId;
	}

	public void setCurUserId(Long curUserId) {
		this.curUserId = curUserId;
	}

	public String getNextUser() {
		return nextUser;
	}

	public void setNextUser(String nextUser) {
		this.nextUser = nextUser;
	}

	public BigDecimal getProposalAmt() {
		return proposalAmt;
	}

	public void setProposalAmt(BigDecimal proposalAmt) {
		this.proposalAmt = proposalAmt;
	}

	public Integer getProposalTerm() {
		return proposalTerm;
	}

	public void setProposalTerm(Integer proposalTerm) {
		this.proposalTerm = proposalTerm;
	}

	public String getProposalTermUnit() {
		return proposalTermUnit;
	}

	public void setProposalTermUnit(String proposalTermUnit) {
		this.proposalTermUnit = proposalTermUnit;
	}

	public BigDecimal getProposalRate() {
		return proposalRate;
	}

	public void setProposalRate(BigDecimal proposalRate) {
		this.proposalRate = proposalRate;
	}

	public String getComments() {
		return comments;
	}

	public void setComments(String comments) {
		this.comments = comments;
	}

	public String getCurUserLoginName() {
		return curUserLoginName;
	}

	public void setCurUserLoginName(String curUserLoginName) {
		this.curUserLoginName = curUserLoginName;
	}

	public String getTaskStageCode() {
		return taskStageCode;
	}

	public void setTaskStageCode(String taskStageCode) {
		this.taskStageCode = taskStageCode;
	}

	public Long getCurUserOrgId() {
		return curUserOrgId;
	}

	public void setCurUserOrgId(Long curUserOrgId) {
		this.curUserOrgId = curUserOrgId;
	}

	public Long getCurUserRoleId() {
		return curUserRoleId;
	}

	public void setCurUserRoleId(Long curUserRoleId) {
		this.curUserRoleId = curUserRoleId;
	}

	public String getNextUserOrgId() {
		return nextUserOrgId;
	}

	public void setNextUserOrgId(String nextUserOrgId) {
		this.nextUserOrgId = nextUserOrgId;
	}

	public String getCurUserName() {
		return curUserName;
	}

	public void setCurUserName(String curUserName) {
		this.curUserName = curUserName;
	}


}
